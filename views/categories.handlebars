<body class="body3">
    <nav class="navbar navbar-expand-lg fixed-top ">
      <a class="navbar-brand" href="/">My-Meme</a>
      <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarText"
        aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
        <span class="icon-bar top-bar"></span>
        <span class="icon-bar middle-bar"></span>
        <span class="icon-bar bottom-bar"></span>
      </button>
      <div class="collapse navbar-collapse " id="navbarText">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active ">
            <a class="nav-link" href="/popularMemes">Popular Memes</a>
          </li>
        <li class="nav-item">
          <a class="nav-link" href="/categories">Catagories</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/allMemes">All Memes</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/addAMeme">Add A Meme</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/team">Team</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/memeWars" target="blank_">Meme War</a>
        </li>      
      </ul>
      <form class="form-inline my-2 my-lg-0">
        <input class="form-control mr-sm-2" id="meme-search" type="search" placeholder="Search" aria-label="Search">
        <button class="btn mb-2 my-sm-0" type="submit" id="search-btn">Search</button>
      </form>
       </div>
  </nav>
  <br><br><br><br>

  <div class="container-fluid">
    <div class="container">
      <div class="row">
        <div id="catBG" class="col-sm-8 text-white ">
          <h1 class="catH">Categories</h1>
        </div>
        <div class="col-sm-4">
          <div class="form-group">
            <div class="row">
              <div class="col">
                <select class="form-control" id="categorySelect">
                  <option value="Political">Political</option>
                  <option value="Dank">Dank</option>
                  <option value="Dank Christian">Dank Christian</option>
                  <option value="Animal">Animal</option>
                  <option value="Facebook">Facebook</option>
                </select>
              </div>
              <div class="col">
                <button id="FormCategory1" type="submit" class="btn">Submit</button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="container" id="meme-container">
      <div class="row text-center">
        {{#each Meme}}
        {{> memes/meme-cat}}
        {{/each}}
      </div>
    </div>
  </div>
  
 </body>